<% title t('.page_title') %>

<div class="grid-row">
  <div class="column-two-thirds">
    <%= step_header %>

    <h1 class="heading-xlarge gv-u-heading-xxlarge"><%=t '.heading' %></h1>

    <%= step_form @form_object do |f| %>
      <%= f.text_field :number_of_children, type: :number, class: 'form-control' %>
      <%= f.submit class: 'button' %>
    <% end %>
  </div>
</div>
