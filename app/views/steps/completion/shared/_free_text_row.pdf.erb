<tr id="<%= free_text_row.question %>">
  <td class="question">
    <%=t "check_answers.#{free_text_row.question}.question" %>
  </td>

  <td class="answer answer-value">
    <%= simple_format(
      free_text_row.value.presence || t("check_answers.#{free_text_row.question}.absence_answer")
    ) %>
  </td>
</tr>
