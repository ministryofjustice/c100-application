<tr id="<%= free_text_row.question %>">
  <td class="question">
    <%=t "check_answers.#{free_text_row.question}.question", free_text_row.i18n_opts %>
  </td>

  <td class="answer answer-value">
    <span class='user-free-text'>
      <%= free_text_row.value.presence || simple_format(
        t("check_answers.#{free_text_row.question}.absence_answer")
      ) %>
    </span>
  </td>
</tr>
