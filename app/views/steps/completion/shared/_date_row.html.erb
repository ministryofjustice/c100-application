<dl id="<%= date_row.question %>">
  <dt class="question">
    <%=t "check_answers_html.#{date_row.question}.question" %>
  </dt>
  <dd class="answer">
    <%= l(date_row.value) %>
  </dd>
  <dd class="change">
    <%= render partial: 'steps/completion/shared/change_link', locals: { question: date_row } %>
  </dd>
</dl>
